<h2 class="ui header">Developers</h2>
{{#link-to "developers.new" class="ui button orange"}}Add new developer...{{/link-to}}
<table class="ui single line table">
  <thead>
  <tr>
    <th>Identity</th>
  </tr>
  </thead>
  <tbody>
  <th>
  {{#each model as |dev|}}
    <tr>
      <td>{{dev.identity}}</td>
      <i class="large icons">
        {{#link-to "developers.update" dev}}<i class="edit outline icon" ></i>{{/link-to}}
      </i>
      &nbsp;&nbsp;
      <i class="large icons">
        <i {{action "delete" dev}} class="window close outline icon"></i>
      </i>
    </tr>
  {{/each}}
  </th>
  </tbody>
</table>

{{outlet}}